# Port Configuration
OPENWEBUI_PORT=3000

# Ollama Configuration
OLLAMA_BASE_URL=http://ollama-container:11434
# OLLAMA_BASE_URLS=http://ollama-container-1:11434;http://ollama-container-2:11434

# Authentication & User Management
WEBUI_AUTH=True
ENABLE_API_KEY=True
ENABLE_SIGNUP=True
DEFAULT_USER_ROLE=pending
ENABLE_ADMIN_CHAT_ACCESS=False

# Feature Toggles
K8S_FLAG=False
ENABLE_OPENAI_API=False
ENABLE_CODE_EXECUTION=False
ENABLE_CODE_INTERPRETER=False
ENABLE_API_KEY_ENDPOINT_RESTRICTIONS=True
ENABLE_ONEDRIVE_INTEGRATION=False
ENABLE_WEB_SEARCH=False
OFFLINE_MODE=False

# File & Search Configuration
RAG_ALLOWED_FILE_EXTENSIONS=["pdf,docx,txt,py,js,ps1,psm1,psd1,c,cpp,ts"]
WEB_SEARCH_ENGINE=duckduckgo

# User Permissions
USER_PERMISSIONS_WORKSPACE_MODELS_ACCESS=True
USER_PERMISSIONS_WORKSPACE_KNOWLEDGE_ACCESS=True
USER_PERMISSIONS_WORKSPACE_PROMPTS_ACCESS=False
USER_PERMISSIONS_WORKSPACE_TOOLS_ACCESS=False

# Optional OAuth/Database (uncomment as needed)
# MICROSOFT_CLIENT_ID=your_microsoft_client_id
# GITHUB_CLIENT_ID=your_github_client_id
# OAUTH_CLIENT_ID=your_oauth_client_id
# DATABASE_URL=your_database_url
